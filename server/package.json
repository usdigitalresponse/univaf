{
  "name": "appointment-availability-server",
  "version": "0.1.0",
  "description": "A server which logs various appointment availability",
  "repository": {
    "type": "git",
    "url": "https://github.com/usdigitalresponse/appointment-availability-infra"
  },
  "author": "USDR",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "build-ts": "tsc",
    "build": "npm run build-ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/src/server.js",
    "serve": "node dist/src/server.js",
    "start": "npm run serve",
    "test": "jest --forceExit --coverage --verbose --detectOpenHandles",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/src/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "db:seed": "npm run build && node dist/scripts/seed-database.js",
    "knex": "knex",
    "migrate": "knex migrate:latest",
    "migrate:up": "knex migrate:up",
    "migrate:down": "knex migrate:down",
    "migrate:list": "knex migrate:list"
  },
  "jest": {
    "globals": {
      "NODE_ENV": "test"
    }
  },
  "dependencies": {
    "@sentry/node": "^6.2.5",
    "@sentry/tracing": "^6.2.5",
    "async": "3.2.0",
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dotenv": "8.2.0",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "express-validator": "6.9.2",
    "knex": "^0.95.4",
    "lodash": "4.17.20",
    "pg": "^8.5.1",
    "winston": "3.3.3"
  },
  "devDependencies": {
    "@types/async": "3.2.5",
    "@types/bluebird": "3.5.33",
    "@types/body-parser": "1.19.0",
    "@types/chai": "4.2.14",
    "@types/compression": "1.7.0",
    "@types/concurrently": "5.2.1",
    "@types/cors": "^2.8.10",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "7.2.6",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.20",
    "@types/lodash": "4.14.168",
    "@types/node": "14.14.25",
    "@types/pg": "^7.14.11",
    "@types/request": "2.48.5",
    "@types/request-promise": "4.1.47",
    "@types/shelljs": "0.8.8",
    "@types/supertest": "2.0.10",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "chai": "4.3.0",
    "concurrently": "5.3.0",
    "eslint": "7.23.0",
    "jest": "26.6.3",
    "nodemon": "2.0.7",
    "prettier": "^2.2.1",
    "shelljs": "0.8.4",
    "supertest": "6.1.3",
    "ts-jest": "26.5.0",
    "ts-node": "9.1.1",
    "typescript": "4.1.3"
  }
}
