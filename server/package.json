{
  "name": "univaf-server",
  "version": "0.1.0",
  "description": "An API server that stores and provides COVID-19 vaccine appointment availability data.",
  "repository": {
    "type": "git",
    "url": "https://github.com/usdigitalresponse/univaf"
  },
  "author": "USDR",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "build-ts": "tsc",
    "build": "npm run build-ts",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon --inspect dist/src/server.js",
    "serve": "node dist/src/server.js",
    "start": "npm run serve",
    "test": "NODE_ENV=test jest --coverage --verbose --detectOpenHandles",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "watch-node": "nodemon dist/src/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run watch-node\"",
    "db:seed": "npm run build && node dist/scripts/seed-database.js",
    "knex": "knex",
    "migrate": "knex migrate:latest",
    "migrate:up": "knex migrate:up",
    "migrate:down": "knex migrate:down",
    "migrate:list": "knex migrate:list",
    "lint": "eslint --ext .js,.ts ."
  },
  "dependencies": {
    "@sentry/node": "^6.13.3",
    "@sentry/tracing": "^6.13.3",
    "ajv": "^8.6.3",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.0.0",
    "async": "3.2.1",
    "aws-sdk": "^2.1010.0",
    "bluebird": "3.7.2",
    "body-parser": "1.19.0",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dotenv": "10.0.0",
    "errorhandler": "1.5.1",
    "express": "4.17.1",
    "hot-shots": "^8.5.2",
    "JSONStream": "^1.3.5",
    "knex": "^0.95.11",
    "lodash": "4.17.21",
    "luxon": "^1.27.0",
    "pg": "^8.7.1",
    "pg-query-stream": "^4.1.0",
    "winston": "3.3.3",
    "yargs": "^17.2.1"
  },
  "devDependencies": {
    "@types/async": "3.2.7",
    "@types/bluebird": "3.5.36",
    "@types/body-parser": "1.19.1",
    "@types/chai": "4.2.21",
    "@types/compression": "1.7.1",
    "@types/concurrently": "6.2.1",
    "@types/cors": "^2.8.11",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "7.28.0",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/lodash": "4.14.171",
    "@types/luxon": "^1.27.1",
    "@types/node": "14.14.31",
    "@types/pg": "^8.6.1",
    "@types/request": "2.48.6",
    "@types/request-promise": "4.1.48",
    "@types/shelljs": "0.8.9",
    "@types/winston": "2.4.4",
    "@typescript-eslint/eslint-plugin": "4.32.0",
    "@typescript-eslint/parser": "4.32.0",
    "chai": "4.3.4",
    "concurrently": "6.2.0",
    "eslint": "7.32.0",
    "got": "^11.8.2",
    "jest": "27.2.4",
    "nodemon": "2.0.10",
    "prettier": "^2.4.1",
    "shelljs": "0.8.4",
    "ts-jest": "27.0.5",
    "ts-node": "10.0.0",
    "typescript": "4.3.5"
  }
}
