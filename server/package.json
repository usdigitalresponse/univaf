{
  "name": "univaf-server",
  "version": "0.1.0",
  "description": "An API server that stores and provides COVID-19 vaccine appointment availability data.",
  "repository": {
    "type": "git",
    "url": "https://github.com/usdigitalresponse/univaf"
  },
  "author": "USDR",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "build-ts": "tsc",
    "build": "npm run build-ts",
    "debug": "npm run build && npm run watch-debug",
    "serve": "node dist/src/server.js",
    "start": "npm run serve",
    "test": "NODE_ENV=test jest --coverage --verbose --detectOpenHandles",
    "watch": "concurrently -k -n 'TypeScript,Node' -c 'yellow.bold,cyan.bold' 'npm run watch-ts' 'npm run watch-node'",
    "watch-debug": "concurrently -k -n 'TypeScript,Node' -c 'yellow.bold,cyan.bold' 'npm run watch-ts' 'npm run watch-node-debug'",
    "watch-node": "nodemon dist/src/server.js",
    "watch-node-debug": "nodemon --inspect dist/src/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc -w",
    "db:seed": "npm run build && node dist/scripts/seed-database.js",
    "knex": "knex",
    "migrate": "knex migrate:latest",
    "migrate:up": "knex migrate:up",
    "migrate:down": "knex migrate:down",
    "migrate:list": "knex migrate:list",
    "lint": "eslint --ext .js,.ts ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.179.0",
    "@aws-sdk/lib-storage": "^3.179.0",
    "@sentry/node": "^7.13.0",
    "@sentry/tracing": "^7.13.0",
    "ajv": "^8.11.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "body-parser": "^1.20.0",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dotenv": "16.0.2",
    "errorhandler": "1.5.1",
    "express": "4.18.1",
    "hot-shots": "^9.2.0",
    "http-terminator": "^3.2.0",
    "JSONStream": "^1.3.5",
    "knex": "^2.3.0",
    "lodash": "4.17.21",
    "luxon": "^3.0.4",
    "pg": "^8.8.0",
    "pg-query-stream": "^4.2.4",
    "winston": "3.8.2",
    "yargs": "^17.5.1"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/compression": "1.7.2",
    "@types/cors": "^2.8.12",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "8.4.6",
    "@types/express": "4.17.14",
    "@types/jest": "29.0.3",
    "@types/lodash": "4.14.185",
    "@types/luxon": "^3.0.1",
    "@types/node": "16.11.59",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "5.38.1",
    "@typescript-eslint/parser": "5.38.1",
    "concurrently": "7.4.0",
    "eslint": "^8.24.0",
    "got": "^11.8.5",
    "jest": "^29.0.3",
    "nodemon": "2.0.20",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.1",
    "ts-node": "10.9.1",
    "typescript": "4.8.3"
  }
}
