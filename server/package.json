{
  "private": true,
  "name": "univaf-server",
  "version": "0.1.0",
  "description": "An API server that stores and provides COVID-19 vaccine appointment availability data.",
  "repository": {
    "type": "git",
    "url": "https://github.com/usdigitalresponse/univaf"
  },
  "author": "USDR",
  "license": "MIT",
  "scripts": {
    "clean": "rm -rf dist node_modules",
    "build-ts": "tsc --build",
    "build": "npm run build-ts",
    "debug": "npm run build && npm run watch-debug",
    "serve": "node dist/src/server.js",
    "start": "npm run serve",
    "test": "NODE_ENV=test jest --coverage --verbose --detectOpenHandles",
    "watch": "concurrently -k -n 'TypeScript,Node' -c 'yellow.bold,cyan.bold' 'npm run watch-ts' 'npm run watch-node'",
    "watch-debug": "concurrently -k -n 'TypeScript,Node' -c 'yellow.bold,cyan.bold' 'npm run watch-ts' 'npm run watch-node-debug'",
    "watch-node": "nodemon dist/src/server.js",
    "watch-node-debug": "nodemon --inspect dist/src/server.js",
    "watch-test": "npm run test -- --watchAll",
    "watch-ts": "tsc --build --watch",
    "db:seed": "npm run build && node dist/scripts/seed-database.js",
    "knex": "knex",
    "migrate": "knex migrate:latest",
    "migrate:up": "knex migrate:up",
    "migrate:down": "knex migrate:down",
    "migrate:list": "knex migrate:list",
    "lint": "eslint --ext .js,.ts ."
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.295.0",
    "@aws-sdk/lib-storage": "^3.300.0",
    "@sentry/node": "^7.44.1",
    "@sentry/tracing": "^7.44.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ajv-keywords": "^5.1.0",
    "body-parser": "^1.20.2",
    "compression": "1.7.4",
    "cors": "^2.8.5",
    "dotenv": "16.0.3",
    "errorhandler": "1.5.1",
    "express": "4.18.2",
    "hot-shots": "^10.0.0",
    "http-terminator": "^3.2.0",
    "JSONStream": "^1.3.5",
    "knex": "^2.4.2",
    "lodash": "4.17.21",
    "luxon": "^3.3.0",
    "pg": "^8.10.0",
    "pg-query-stream": "^4.4.0",
    "univaf-common": ">=0.1.0",
    "winston": "3.8.2",
    "yargs": "^17.7.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.5.0",
    "@types/body-parser": "1.19.2",
    "@types/compression": "1.7.2",
    "@types/cors": "^2.8.13",
    "@types/errorhandler": "1.5.0",
    "@types/eslint": "8.21.3",
    "@types/express": "4.17.17",
    "@types/lodash": "4.14.191",
    "@types/luxon": "^3.2.0",
    "@types/node": "18.15.5",
    "@types/pg": "^8.6.6",
    "concurrently": "7.6.0",
    "got": "^11.8.6",
    "jest": "^29.5.0",
    "nodemon": "2.0.21",
    "ts-jest": "^29.0.5",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
}
